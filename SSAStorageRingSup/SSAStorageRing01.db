    field(DOL, "RA-ToSIA01:AlarmConfig:GeneralPowerLimLoLo CP")
    field(OUT, "RA-ToSIA01:RF-SSAmpTower:PwrRevIn4-Mon.LOLO")

    field(FLNK, "RA-ToSIA01:RF-SSAmpTower:PwrRevIn4-Mon")
}

record(scalcout, "RA-ToSIA01:RF-SSAmpTower:PwrRevIn4-Mon_v"){
    field(CALC, "(DBL(AA[4,7])/4095.0) * 5.0")
    field(INAA, "$(P)$(R)RACK4RawData-Mon.VAL[80] CP MSS")

    field(EGU,  "V")
}

record(calc, "RA-ToSIA01:RF-SSAmpTower:PwrRevIn4-Mon"){
    field(CALC, "(10 * LOG((11.4*(A*A) + 1.7*A + 0.01))) + B")
    field(INPA, "RA-ToSIA01:RF-SSAmpTower:PwrRevIn4-Mon_v CP MSS")
    field(INPB, "RA-ToSIA01:OffsetConfig:InputReflectedPower CP ")

    field(EGU,  "dBm")
    field(DESC, "Potência Geral")
    field(PREC, "2")

    field(HHSV, "MAJOR")
    field(HSV,  "MINOR")
    field(LSV,  "MINOR")
    field(LLSV, "MAJOR")
}

